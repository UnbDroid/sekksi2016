#define GARRA OUT_A
#define BttnGarraFecha IN_1
#define BttnGarraAbre IN_2



task main()
{

     SetSensorTouch(BttnGarraFecha);
     SetSensorTouch(BttnGarraAbre);
     while(true)
     {
      if(SensorBoolean(BttnGarraFecha)){
        OnFwd(GARRA,30);
        Wait(50);
      }
      else if(SensorBoolean(BttnGarraAbre)){
        OnRev(GARRA,50);
        Wait(50);
        }
     }
}
